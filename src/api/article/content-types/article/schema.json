{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": "Create your blog content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "text",
      "maxLength": 80
    },
    "slug": {
      "type": "uid"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "name": {
      "type": "string"
    },
    "tag": {
      "type": "string"
    },
    "available": {
      "type": "boolean"
    },
    "expansion": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::collection.collection",
      "inversedBy": "name"
    },
    "type": {
      "type": "string"
    },
    "code": {
      "type": "string"
    },
    "images": {
      "type": "component",
      "repeatable": true,
      "component": "shared.slider"
    }
  }
}
